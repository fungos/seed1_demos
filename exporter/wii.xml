<root>
	<strings>
		<dictionary language='en_US'>
			<string name='helloworld' text='Hello World %ls!' />
			<string name='enabled' text='Enabled' />
			<string name='disabled' text='Disabled' />
		</dictionary>
		<dictionary language='pt_BR'>
			<string name='helloworld' text='%ls Olá, mundo!' />
			<string name='enabled' text='Habilitado' />
			<string name='disabled' text='Desabilitado' />
		</dictionary>
	</strings>
	<resources>
		<resource type='image' filename='sprite/basic1/seed_logo' name='seed_logo' />
		<resource type='image' filename='sprite/movie1/arrow' name='arrow' />
		<resource type='image' filename='sprite/movie3/spotlight' name='spotlight' />
		<resource type='image' filename='sprite/movie5/witchdoctor_body' name='witchdoctor_body' />
		<resource type='image' filename='sprite/movie5/witchdoctor_upper_body' name='witchdoctor_upper_body' />
		<resource type='image' filename='sprite/movie5/witchdoctor_head' name='witchdoctor_head' />
		<resource type='image' filename='gui/basic1/btn_disabled' name='btn_disabled' />
		<resource type='image' filename='gui/basic1/btn_out' name='btn_out' />
		<resource type='image' filename='gui/basic1/btn_over' name='btn_over' />
		<resource type='image' filename='gui/basic1/btn_pressed' name='btn_pressed' />
		
		<resource type='image' filename='gui/basic3/font25' name='font25' />
		<resource type='image' filename='gui/basic3/font25_ext' name='font25_ext' language='pt_BR' />

		<resource type='mask' filename='gui/basic2/btn_mask' name='btn_mask' />
		
		<resource type='sound' filename='sound/basic1/helloworld' name='helloworld' />
		<resource type='sound' filename='sound/basic1/pickup_item' name='pickup_item' />
		
		<resource type='music' filename='sound/basic1/theme' name='theme' />
	</resources>
	<objects>
		<object type='sprite' name='sprite/basic1/seed_logo' group='demo'>
			<animation name='default'>
				<frame resource='seed_logo' />
			</animation>
		</object>
		<object type='sprite' name='sprite/movie1/arrow' group='demo'>
			<animation name='default'>
				<frame resource='arrow' />
			</animation>
		</object>
		<object type='sprite' name='sprite/movie3/spotlight' group='demo'>
			<animation name='default'>
				<frame resource='spotlight' />
			</animation>
		</object>
		<object type='sprite' name='sprite/movie5/witchdoctor_body' group='demo'>
			<animation name='default'>
				<frame resource='witchdoctor_body' />
			</animation>
		</object>
		<object type='sprite' name='sprite/movie5/witchdoctor_upper_body' group='demo'>
			<animation name='default'>
				<frame resource='witchdoctor_upper_body' />
			</animation>
		</object>
		<object type='sprite' name='sprite/movie5/witchdoctor_head' group='demo'>
			<animation name='default'>
				<frame resource='witchdoctor_head' />
			</animation>
		</object>
		<object type='sprite' name='gui/basic1/btn_sprite' group='demo'>
			<animation name='idle'>
				<frame resource='btn_out' />
			</animation>
			<animation name='rollout'>
				<frame resource='btn_out' />
			</animation>
			<animation name='rollover'>
				<frame resource='btn_over' />
			</animation>
			<animation name='disabled'>
				<frame resource='btn_disabled' />
			</animation>
			<animation name='selected'>
				<frame resource='btn_pressed' />
			</animation>
			<animation name='press'>
				<frame resource='btn_pressed' />
			</animation>
			<animation name='release'>
				<frame resource='btn_over' />
			</animation>
		</object>
		
		<object type='button' name='gui/basic2/btn_mask' id='1' object_sprite='gui/basic1/btn_sprite' x='0.44' y='0.6' priority='1' label='botao' collision='mask' resource_mask='btn_mask' group='demo' />
		
		<object type='sound' name='sound/basic1/helloworld' resource='helloworld' loop='true' volume='100' group='demo' />
		<object type='sound' name='sound/basic1/pickup_item' resource='pickup_item' loop='false' volume='100' group='demo' />
		
		<object type='music' name='sound/basic1/theme' resource='theme' volume='20' group='demo' />
		
		<object type='font' name='gui/basic3/font25' spacing='0' tracking='0.01' space='0.03' resource='font25' group='common' />
	</objects>
</root>